<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="action_overlay_test_item" model="ir.actions.act_window">
    <field name="name">Overlay Test Items</field>
    <field name="res_model">overlay.test.item</field>
    <field name="view_mode">tree,form</field>
  </record>

  <menuitem id="menu_overlay_test_item"
            name="Test Items"
            parent="menu_pre_printed_forms_root"
            action="action_overlay_test_item"
            sequence="20"/>

  <record id="view_overlay_test_item_tree" model="ir.ui.view">
    <field name="name">overlay.test.item.tree</field>
    <field name="model">overlay.test.item</field>
    <field name="arch" type="xml">
      <tree>
        <field name="name"/>
        <field name="form_id"/>
        <field name="config_id"/>
      </tree>
    </field>
  </record>

  <record id="view_overlay_test_item_form" model="ir.ui.view">
    <field name="name">overlay.test.item.form</field>
    <field name="model">overlay.test.item</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <field name="name"/>
            <field name="form_id"/>
            <field name="notes"/>
            <field name='x'/>
            <field name='y'/>
            <field name='text'/>
            <field name="config_id"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>
</odoo>
